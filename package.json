{
  "name": "aws-lambda-typescript-3tier-boilerplate",
  "version": "0.0.1",
  "description": "aws-lambda-typescript-3tier-boilerplate",
  "scripts": {
    "build": "webpack",
    "deploy": "cdk deploy --context awsEnv=$AWS_ENV '*'",
    "lint:ec": "ec --exclude 'node_modules|dist|\\.(git|vscode)'",
    "lint:eslint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:prettier": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run lint:ec && npm run lint:eslint",
    "clean": "rimraf dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:up": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "cdk": "cdk"
  },
  "private": true,
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/bunyan": "^1.8.6",
    "@types/chance": "^1.1.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/node-rsa": "^1.0.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "aws-cdk": "^1.54.0",
    "chance": "^1.1.6",
    "editorconfig-checker": "^3.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.3",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.54.0",
    "@aws-cdk/aws-apigatewayv2": "^1.54.0",
    "@aws-cdk/aws-certificatemanager": "^1.54.0",
    "@aws-cdk/aws-dynamodb": "^1.54.0",
    "@aws-cdk/aws-route53": "^1.54.0",
    "@aws-cdk/aws-route53-targets": "^1.54.0",
    "@aws-cdk/core": "^1.54.0",
    "aws-sdk": "^2.814.0",
    "bunyan": "^1.8.14"
  }
}
